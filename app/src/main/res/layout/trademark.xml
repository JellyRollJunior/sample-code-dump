<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <ImageView
        android:id="@+id/ivSample31"
        android:layout_width="22dp"
        android:layout_height="22dp"
        android:src="@drawable/animal_crossing_leaf"
        android:contentDescription="@string/content_description_sample_4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/developed_by_me"
        android:textSize="16sp"
        android:layout_marginStart="3dp"
        app:layout_constraintStart_toEndOf="@+id/ivSample31"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>